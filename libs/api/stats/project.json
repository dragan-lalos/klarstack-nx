{
  "name": "api-stats",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/stats/src",
  "projectType": "library",
  "tags": ["scope:api", "type:feature", "domain:stats"],
  "targets": {
    "tsc:typecheck": {
      "executor": "nx:run-commands",
      "dependsOn": ["^tsc:typecheck"],
      "options": {
        "cwd": "libs/api/stats",
        "command": "tsc -p tsconfig.lib.json --noEmit"
      }
    },
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["api-tenancy:build", "api-auth:build", "tsc:typecheck"],
      "options": {
        "outputPath": "dist/libs/api/stats",
        "main": "libs/api/stats/src/index.ts",
        "tsConfig": "libs/api/stats/tsconfig.lib.json",
        "assets": [],
        "skipTypeCheck": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/api/stats/jest.config.cts",
        "passWithNoTests": true
      }
    }
  }
}
