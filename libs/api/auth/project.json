{
  "name": "api-auth",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/auth/src",
  "projectType": "library",
  "tags": ["scope:api", "type:feature", "domain:auth"],
  "targets": {
    "tsc:typecheck": {
      "executor": "nx:run-commands",
      "dependsOn": ["^tsc:typecheck"],
      "options": {
        "cwd": "libs/api/auth",
        "command": "tsc -p tsconfig.lib.json --noEmit"
      }
    },
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["api-users:build", "tsc:typecheck"],
      "options": {
        "outputPath": "dist/libs/api/auth",
        "main": "libs/api/auth/src/index.ts",
        "tsConfig": "libs/api/auth/tsconfig.lib.json",
        "assets": [],
        "skipTypeCheck": true
      }
    }
  },
  "lint": {
    "executor": "@nx/eslint:lint"
  },
  "test": {
    "executor": "@nx/jest:jest",
    "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
    "options": {
      "jestConfig": "libs/api/auth/jest.config.cts",
      "passWithNoTests": true
    }
  }
}
